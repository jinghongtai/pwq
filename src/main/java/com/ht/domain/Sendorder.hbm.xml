<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ht.domain.Sendorder" table="sendorder" schema="pwq">
        <id name="id" column="id"/>
        <property name="orderNo" column="orderNo"/>
        <property name="areaId" column="areaId"/>
        <property name="pwqId" column="pwqId"/>
        <property name="orderState" column="orderState"/>
        <property name="clientIp" column="clientIp"/>
        <property name="sendTime" column="sendTime"/>
        <property name="fanOperation" column="fanOperation"/>
        <property name="sprayOperation" column="sprayOperation"/>
        <property name="galeStrategy" column="galeStrategy"/>
        <property name="swingOperation" column="swingOperation"/>
        <property name="galeclFx" column="galeclFx"/>
        <property name="swingFd" column="swingFd"/>
    </class>
</hibernate-mapping>